{
  "name": "house-auction",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{ts,vue}\" --ignore-pattern \"src/_shared/components/storyblok/schema\"",
    "signalr:update-types": "dotnet tsrts --project ../backend/src/HouseAuction/HouseAuction.csproj --output ./src/_shared/providers/generated",
    "storyblok:update-types": "npm run storyblok:pull-components && npm run storyblok:generate-types",
    "storyblok:pull-components": "storyblok pull-components --space 298738 --path src/_shared/components/storyblok/schema/",
    "storyblok:generate-types": "storyblok generate-typescript-typedefs --sourceFilePaths ./src/_shared/components/storyblok/schema/components.298738.json --destinationFilePath ./src/_shared/components/storyblok/schema/component-types.d.ts"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@storyblok/vue": "^8.0.8",
    "@vee-validate/yup": "^4.13.2",
    "house-auction": "file:",
    "tailwindcss-animated": "^1.1.2",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.40",
    "storyblok": "^3.32.3",
    "tailwindcss": "^3.4.7",
    "typescript": "~5.4.0",
    "typescript-eslint": "^7.17.0",
    "unplugin-vue-router": "^0.10.1",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
